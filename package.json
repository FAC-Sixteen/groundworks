{
  "name": "client",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less|sass|scss)$": "<rootDir>/__mocks__/styleMock.js",
      "\\.(gif|ttf|eot|svg)$": "<rootDir>/__mocks__/fileMock.js"
    }
  },
  "scripts": {
    "test": "jest",
    "dev": "webpack-dev-server --open --hot --mode=development",
    "start": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js -d --config ./webpack.config.js --content-base public/ --progress --colors ",
    "build": "webpack --mode=production",
    "heroku-prebuild": "yarn install --dev",
    "heroku-postbuild": "yarn install && yarn run build",
    "sass": "sass --watch src/style/sass:src/style/css"
  },
  "engines": {
    "yarn": "1.16.0"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "clean-webpack-plugin": "3.0.0",
    "node": "^12.4.0",
    "prop-types": "15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.0",
    "webpack": "4.35.0",
    "webpack-cli": "3.3.5",
    "webpack-dev-server": "3.7.2"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@testing-library/react": "^8.0.1",
    "babel-eslint": "10.0.1",
    "babel-loader": "^8.0.6",
    "babel-polyfill": "^6.26.0",
    "css-loader": "^2.1.1",
    "enzyme": "^3.10.0",
    "eslint": "5.16.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-plugin-import": "2.17.3",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-react": "7.13.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "3.2.0",
    "jest": "^24.8.0",
    "mini-css-extract-plugin": "0.7.0",
    "style-loader": "^0.23.1",
    "supertest": "^4.0.2"
  }
